{
  "name": "focus-extension",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "concurrently \"vite build --watch\" \"vite build --config vite.config.confetti.ts --watch\"",
    "build": "vite build && vite build --config vite.config.confetti.ts",
    "style": "prettier --check \"src/**/*.{ts,tsx}\"",
    "style:fix": "prettier --write \"src/**/*.{ts,tsx,css}\""
  },
  "author": "Yuri Luiz",
  "license": "MIT",
  "dependencies": {
    "canvas-confetti": "^1.9.3",
    "classnames": "^2.5.1",
    "framer-motion": "^11.3.19",
    "node": "^20.16.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.0",
    "react-router-dom": "^6.25.1"
  },
  "prettier": {
    "tabWidth": 4,
    "singleQuote": true,
    "printWidth": 100
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.6.4",
    "@types/chrome": "0.0.158",
    "@types/node": "^20.14.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "husky": "^8.0.3",
    "i18next": "^23.12.2",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.40",
    "prettier": "^2.8.8",
    "react-icons": "^5.2.1",
    "shell-quote": "^1.8.1",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "vite": "^5.3.5"
  }
}
